{
  "name": "coding-interview-api",
  "description": "Coding Interview API",
  "version": "0.0.0",
  "main": "app.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "start": "cross-env NODE_ENV=development node dist/src/server.js",
    "start-prod": "cross-env NODE_ENV=production node dist/src/server.js",
    "generate-oai-html": "cd generated && redoc-cli bundle swagger.json && node ../utils/copy.js redoc-static.html openapi.html && cd ..",
    "build": "tsoa spec-and-routes && yarn generate-oai-html && tsc",
    "lint": "eslint . --ext .ts,.js,.json",
    "test": "echo ok",
    "test:cov": "echo ok",
    "test:e2e": "echo ok",
    "format": "prettier --write \"(src|test)/**/*.ts\" \"*.md\""
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.0.3",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.4.7",
    "msw": "^0.35.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "redoc-cli": "0.13.2",
    "supertest": "^6.1.5",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "generate-password": "^1.7.0",
    "http-status-codes": "^2.2.0",
    "mongoose": "^6.2.1",
    "multer": "^1.4.4",
    "qs": "^6.10.2",
    "tsoa": "4.0.0-alpha.2",
    "yup": "^0.32.11"
  }
}
